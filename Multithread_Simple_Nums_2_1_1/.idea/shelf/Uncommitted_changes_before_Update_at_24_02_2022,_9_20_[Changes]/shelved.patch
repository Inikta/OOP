+package ru.nsu.nikita.threads;
+
+import java.util.ArrayList;
+import java.util.List;
+
+public class MultiThreadListCheck {
+
+    public static boolean MultiThreadListCheck(int threadsAmount, List<Integer> list) {
+        if (threadsAvailable(threadsAmount)) {
+            for (int i = 0; i % list.size() < list.size(); i += threadsAmount) {
+                List<ThreadSinglePrimeNumberCheck> threads = createThreadsList(threadsAmount);
+                for (int j = 0; j < threadsAmount; j++) {
+                    int finalJ = j;
+                    threads.forEach(t -> t.setNum(list.get(finalJ)));
+                    threads.forEach(ThreadSinglePrimeNumberCheck::start);
+                }
+            }
+
+        }
+    }
+
+    private static boolean threadsAvailable(int threadsAmount) {
+        //условие проверки количества свободных потоков
+        threadsAmount = threadsAmount;
+        return true;
+    }
+
+    private static List<ThreadSinglePrimeNumberCheck> createThreadsList(int amount) {
+        List<ThreadSinglePrimeNumberCheck> threads = new ArrayList<>();
+        for (int i = 0; i < amount; i++) {
+            threads.add(new ThreadSinglePrimeNumberCheck());
+        }
+
+        return threads;
+    }
+
+
+}
